<ArcGIS defaultAssembly="SdmaConnector.dll" defaultNamespace="SdmaConnector" xmlns="http://schemas.esri.com/DADF/Registry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.esri.com/DADF/Registry file:///C:/Program%20Files/ArcGIS/Pro/bin/ArcGIS.Desktop.Framework.xsd">
	<AddInInfo id="{e8c45273-6a7d-4528-b137-c719cb710ab8}" version="1.0.0" desktopVersion="3.5">
		<Name>SdmaConnector</Name>
		<Description>ArcGIS Pro add-in for seamless integration with the Spatial Data Management on AWS solution</Description>
		<Image>Images\AddinDesktop32.png</Image>
		<Author>Esri</Author>
		<Company>Esri, https://www.esri.com</Company>
		<Date>12/1/2025</Date>
		<Subject>Content</Subject>
	</AddInInfo>
	<modules>
		<insertModule id="SdmaConnector_Module" className="Module1" autoLoad="false" caption="Module1">
			<!-- uncomment to have the control hosted on a separate tab-->
			<tabs>
				<!--<tab id="SdmaConnector_Tab1" caption="New Tab">
          <group refID="SdmaConnector_Group1"/>
        </tab>-->
			</tabs>
			<groups>
				<!-- comment this out if you have no controls on the Addin tab to avoid
              an empty group-->
				<group id="SdmaConnector_Group1" caption="SDMA Portal" appearsOnAddInTab="true">
					<!-- host controls within groups -->
					<button refID="SdmaConnector_ProjectExplorer_ShowButton" size="large" />
				</group>
			</groups>
			<controls>
				<!-- add your controls here -->
				<button id="SdmaConnector_ProjectExplorer_ShowButton" caption="Project Explorer" className="ProjectExplorer_ShowButton" loadOnClick="true" smallImage="Images/SdmaIconAlt.png" largeImage="Images/SdmaIconAlt.png">
					<tooltip heading="Show SDMA Portal Dockpane">
						Show SDMA Portal Dockpane<disabledText />
					</tooltip>
				</button>
				<button id="SdmaConnector_OpenSdmaLink_Table" caption="Open SDMA Portal Links" className="SdmaConnector.Commands.OpenSdmaLinkCommand" loadOnClick="true" smallImage="Images/SdmaIconAlt.png" largeImage="Images/SdmaIconAlt.png">
					<tooltip heading="Open SDMA Portal Links">
						Open SDMA file links from selected table rows<disabledText />
					</tooltip>
				</button>
			</controls>
			<dockPanes>
				<dockPane id="SdmaConnector_ProjectExplorer" caption="SDMA Portal"
						  className="ProjectExplorerViewModel" dock="group" dockWith="esri_core_contentsDockPane">
					<content className="ProjectExplorerView" />
				</dockPane>
			</dockPanes>

		</insertModule>
		<updateModule refID="esri_editing_EditingModule">
			<menus>
				<updateMenu refID="esri_editing_tableAllRowContextMenu">
					<insertButton refID="SdmaConnector_OpenSdmaLink_Table" separator="true" />
				</updateMenu>
			</menus>
		</updateModule>
	</modules>
</ArcGIS>